{
  "TRANSFERTASK_HTTP_PORT": 8085,
  "TRANSFERTASK_JWT_AUTH": true,
  "TRANSFERTASK_JWT_VERIFY": false,
  "TRANSFERTASK_JWT_PRIVATE_KEY": "src/test/resources/private_key.pem",
  "TRANSFERTASK_JWT_PUBLIC_KEY": "src/test/resources/public_key.pem",
  "TRANSFERTASK_JDBC_DATA_SOURCE": "org.mariadb.jdbc.MariaDbDataSource",
  "TRANSFERTASK_DB_JDBC_DRIVER_CLASS": "${foundation.db.test.driver}",
  "TRANSFERTASK_DB_JDBC_USERNAME": "${foundation.db.test.username}",
  "TRANSFERTASK_DB_JDBC_PASSWORD": "${foundation.db.test.password}",
  "TRANSFERTASK_DB_JDBC_URL": "${foundation.db.test.url}&serverTimezone=America/Chicago&autoReconnect=True&failOverReadOnly=false&autoReconnectForPools=true&useUnicode=true&characterEncoding=utf8",
  "TRANSFERTASK_DB_JDBC_MAX_POOL_SIZE": 3,
  "TRANSFERTASK_DB_QUEUE": "transfertaskdb.queue",
  "TRANSFERTASK_DB_SQL_QUERIES_RESOURCE_FILE": "src/main/resources/db-queries.yml",
  "TRANSFERTASK_MAX_ATTEMPTS": 1
}