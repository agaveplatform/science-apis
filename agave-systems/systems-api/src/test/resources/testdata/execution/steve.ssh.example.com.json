{
	"id": "steve.ssh.example.com",
	"name": "SSH",
	"status": "UP",
	"type": "EXECUTION",
	"description": "Sample ssh execution system used by Steve",
	"site": "tacc.local",
	"executionType": "CLI",
	"default": true,
	"queues": [
		{
			"name": "normal",
			"maxJobs": 100,
			"maxMemory": "2048GB",
			"customDirectives": "#$ -A TG-MCB110022",
			"default": true
		}
	],
	"login": {
		"host": "${ssh.host}",
		"port": ${ssh.port},
		"protocol": "SSH",
		"scratchDir": "scratch",
		"workDir": "workdir",
		"auth": {
			"username": "testuser",
			"password": "{$default.password}",
			"credential": "",
			"type": "PASSWORD"
		}
	},
	"storage": {
		"host": "${ssh.host}",
		"port": ${ssh.port},
		"protocol": "SFTP",
		"rootDir": "/home/testuser",
		"auth": {
			"username": "testuser",
			"password": "{$default.password}",
			"type": "PASSWORD"
		}
	},
	"scheduler": "FORK",
	"environment": "",
	"startupScript": "./bashrc"
}
