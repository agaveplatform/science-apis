{
  "id": "s3.example.com",
  "name": "Amazon S3 Example",
  "status": "UP",
  "type": "STORAGE",
  "description": "Example storage system accessible via Amazon S3 API.",
  "site": "aws",
  "storage": {
    "host": "${s3.host}",
    "port": ${s3.port},
    "protocol": "S3",
    "homeDir": "/",
    "rootDir": "/",
    "auth": {
      "publicKey": "${s3.access.key}",
      "privateKey": "${s3.secret.key}",
      "type": "APIKEYS"
    },
    "container": "${s3.bucket}"
  },
  "default": false
}